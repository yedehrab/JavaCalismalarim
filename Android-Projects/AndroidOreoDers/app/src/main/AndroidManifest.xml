<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.yunusemre.derscalismao">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_trash_512px"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_trash_512px"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN"
                    android:label="@string/app_name" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".KronometerActivity" />
        <activity android:name=".GameLikeActivity" />
        <activity android:name=".LandmarkCatalogActivity" />
        <activity android:name=".LandmarkCatalogDetailActivity" />
        <activity android:name=".DataBaseActivity" />
        <activity android:name=".FavBookActivity" />
        <activity android:name=".FavBookMenuAddNoteActivity" />
    </application>

</manifest>